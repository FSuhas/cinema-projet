<div class="container" style="display: flex; justify-content: center;">
  <h1>My Books</h1>
</div>

<div class="container" style="display: flex;">
  <% @books.each do |b|%>
    <%= link_to book_path(b) do %>
      <i class="fa-solid fa-images"></i>
      <p><%= b.titre %></p>
    <% end %>
  <% end %>

  <%= link_to new_book_path do %>
    <i class="fa-solid fa-plus" style="font-size: 50px; color: black"></i>
  <% end %>
</div>
<%= render 'shared/retourprofile' %>
<%= console %>
